sourceRelative("make/listener.bsh");
sourceRelative("customize.bsh");

amoStartAll() {
    amoMakeStartListener();
    if (! amoEnableAllCustomizations()) {
        EditBus.sendAsync(amoEBMessage("CUSTOMIZE"));
    }
}

amoStopAll() {
    amoMakeStopListener();
    amoEnableAllCustomizations(false);
}
